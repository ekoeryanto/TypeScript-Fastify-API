{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Eko Eryanto <ekoeryanto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch \"./src/**/*.ts\" --exec ts-node ./src/server.ts",
    "test": "cross-env NODE_ENV=test jest --coverage --verbose",
    "build": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.0.1",
    "fastify-autoload": "^3.0.2",
    "fastify-plugin": "^2.0.0",
    "pino-multi-stream": "^5.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.12.0",
    "@types/pino-multi-stream": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
