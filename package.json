{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Eko Eryanto <ekoeryanto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/server",
    "serve": "tsnd -r tsconfig-paths/register -r dotenv/config --respawn --transpile-only src/server.ts",
    "test": "cross-env NODE_ENV=test jest --coverage --verbose",
    "build": "tsc",
    "lint": "eslint --ext .ts,.js ."
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.0.0",
    "fastify-plugin": "^3.0.0",
    "pino-multi-stream": "^5.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.0.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.0",
    "@types/pino-multi-stream": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "cross-env": "^7.0.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jest": "^26.0.0",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "~4.0.0"
  }
}
